{
    "name": "aon4o/brainfuck-php",
    "description": "A set of tools for BrainFuck for use in a PHP applications.",
    "keywords": [
        "aon4o",
        "brainfuck-php"
    ],
    "homepage": "https://github.com/aon4o/brainfuck-php",
    "license": "MIT",
    "authors": [
        {
            "name": "aon4o",
            "email": "a.o.naidenov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "laravel/pint": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "BrainFuck\\BrainFuck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrainFuck\\BrainFuck\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "lint": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
